Package: ODMeans
Type: Package
Title: OD-Means: k-Means for Origin-Destination
Version: 0.2.0
Authors@R: c(
    person("Sebastian","Moreno",role = c("aut", "cre"),
           email = "sebastian.moreno.araya@gmail.com"),
